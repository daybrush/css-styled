{
    "name": "preact-css-styler",
    "version": "0.3.0",
    "description": "This component is a lightweight, simple line style component.",
    "main": "./dist/styler.cjs.js",
    "module": "./dist/styler.esm.js",
    "sideEffects": false,
    "types": "declaration/index.d.ts",
    "keywords": [
        "styled",
        "inline-style",
        "styled-components",
        "style",
        "css",
        "styler",
        "preact"
    ],
    "dependencies": {
        "@daybrush/utils": "^0.10.0",
        "@types/string-hash": "^1.1.1",
        "preact": "^8.4.2",
        "preact-compat": "^3.19.0",
        "string-hash": "^1.1.3"
    },
    "devDependencies": {
        "@daybrush/builder": "^0.1.1",
        "print-sizes": "0.0.4",
        "react-css-styler": "^0.3.2",
        "rollup-plugin-css-bundle": "^1.0.4",
        "rollup-plugin-preact": "^0.3.0",
        "tslint": "^5.18.0",
        "typescript": "^3.5.2"
    },
    "scripts": {
        "start": "open ./public/index.html && rollup -c rollup.config.demo.js -w",
        "build": "rollup -c && npm run declaration && print-sizes ./dist ",
        "declaration": "rm -rf declaration && tsc -p tsconfig.declaration.json"
    },
    "author": "Daybrush",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/daybrush/css-styler/tree/master/preact-css-styler"
    },
    "bugs": {
        "url": "https://github.com/daybrush/preact-css-styler/issues"
    },
    "homepage": "https://github.com/daybrush/preact-css-styler#readme",
    "eslintConfig": {
        "extends": "preact-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
